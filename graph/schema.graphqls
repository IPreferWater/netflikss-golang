type Serie {
  	directoryName: String!
	label: String!
	seasons: [Season!]
  }

type Season {
	directoryName: String!
	number: Int!
	label: String!
	episodes: [Episode!]
}

type Episode {
	fileName: String!
	number: Int!
	label: String!
}

type Query {
  series: [Serie!]!
}

input InputSerie {
  work: Boolean!
}

type Mutation {
  createSerie(input: InputSerie!): Serie!
  buildSeriesFromInfo(input: Boolean): Boolean!
}