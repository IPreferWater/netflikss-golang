type Serie {
  id: ID!
	label: String!
	Seasons: [Season!]
  }

type Season {
	number: Int!
	label: String!
	episodes: [Episode!]
}

type Episode {
	number: Int!
	label: String!
}

type Query {
  series: [Serie!]!
}

input InputSerie {
  work: Boolean!
}

type Mutation {
  createSerie(input: InputSerie!): Serie!
}